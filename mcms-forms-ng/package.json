{
  "name": "mcms-forms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:mcms-forms",
    "start:mcms-forms": "ng serve --port 4455 --disable-host-check mcms-forms",
    "build": "npm run build:form",
    "lint": "ng lint --fix",
    "build:form": "npm run bundle:form",
    "bundle:form": "ng build --project mcms-form --configuration production --output-hashing none --single-bundle true --plugin ~build-file-names.plugin.js",
    "copy:form": "cpr dist/mcms-form/main.js dist/mcms-form/mcms-form.bundle.js -o",
    "deploy": "bash deploy.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/elements": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/localize": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@mcms-cs/ckeditor5-build-balloon": "^35.2.1-b1",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngx-formly/bootstrap": "^5.10.27",
    "@ngx-formly/core": "^5.10.27",
    "@ungap/custom-elements": "^1.0.0",
    "bootstrap": "^4.6.1",
    "deepmerge": "^4.2.2",
    "fast-deep-equal": "^3.1.3",
    "fast-json-patch": "^3.1.1",
    "ngx-autosize": "^1.8.6",
    "ngx-file-drop": "^13.0.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@types/node": "^16.11.10",
    "codelyzer": "^6.0.0",
    "cpr": "^3.0.1",
    "ngx-build-plus": "^14.0.0",
    "protractor": "~7.0.0",
    "query-string": "^7.0.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.4.0",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4"
  }
}
