{
  "name": "mcms-forms",
  "version": "1.2.6",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:mcms-forms",
    "start:mcms-forms": "ng serve --port 4455 --servePath=mcms-forms --baseHref=/mcms-forms/ --publicHost http://localhost:4455 mcms-forms",
    "build": "npm run build:form",
    "lint": "ng lint --fix",
    "build:form": "npm run bundle:form",
    "bundle:form": "ng build --project mcms-form --prod --output-hashing none --single-bundle true --plugin ~build-file-names.plugin.js",
    "copy:form": "cpr dist/mcms-form/main.js dist/mcms-form/mcms-form.bundle.js -o",
    "deploy": "bash deploy.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
