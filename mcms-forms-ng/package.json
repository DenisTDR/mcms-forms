{
  "name": "mcms-forms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:mcms-forms",
    "start:mcms-forms": "ng serve --port 4455 --servePath=mcms-forms --disable-host-check mcms-forms",
    "build": "npm run build:form",
    "lint": "ng lint --fix",
    "build:form": "npm run bundle:form",
    "bundle:form": "ng build --project mcms-form --configuration production --output-hashing none --single-bundle true --plugin ~build-file-names.plugin.js",
    "copy:form": "cpr dist/mcms-form/main.js dist/mcms-form/mcms-form.bundle.js -o",
    "deploy": "bash deploy.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "^12.0.3",
    "@angular/elements": "^12.0.3",
    "@angular/forms": "~12.0.3",
    "@angular/localize": "^12.0.3",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/router": "~12.0.3",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@mcms-cs/ckeditor5-build-balloon": "^27.1.0",
    "@ng-bootstrap/ng-bootstrap": "^9.1.1",
    "@ngneat/until-destroy": "^8.0.4",
    "@ngx-formly/bootstrap": "^5.10.21",
    "@ngx-formly/core": "^5.10.21",
    "@ungap/custom-elements": "^0.1.15",
    "bootstrap": "^4.6.0",
    "deepmerge": "^4.2.2",
    "fast-deep-equal": "^3.1.3",
    "fast-json-patch": "^3.0.0-1",
    "ngx-autosize": "^1.8.4",
    "ngx-file-drop": "^11.1.0",
    "rxjs": "~6.6.6",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.2",
    "@angular/cli": "^12.0.2",
    "@angular/compiler-cli": "~12.0.3",
    "@types/jasmine": "^3.6.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.5",
    "codelyzer": "^6.0.0",
    "cpr": "^3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-build-plus": "^12.0.1",
    "protractor": "~7.0.0",
    "query-string": "^7.0.1",
    "shelljs": "^0.8.4",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
